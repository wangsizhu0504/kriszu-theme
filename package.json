{
  "publisher": "wangsizhu0504",
  "name": "kriszu-theme",
  "displayName": "Kriszu VScode",
  "type": "module",
  "version": "1.0.16",
  "description": "custom vitesse theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangsizhu0504/kriszu-theme"
  },
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "logo.png",
  "files": [
    "themes"
  ],
  "engines": {
    "vscode": "~1.85.0"
  },
  "preview": true,
  "contributes": {
    "themes": [
      {
        "label": "Kriszu Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/kriszu-dark-soft.json"
      },
      {
        "label": "Kriszu Light",
        "uiTheme": "vs",
        "path": "./themes/kriszu-light.json"
      },
      {
        "label": "Kriszu Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/kriszu-dark.json"
      },
      {
        "label": "Kriszu Light Soft",
        "uiTheme": "vs",
        "path": "./themes/kriszu-light-soft.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "vsce package",
    "release": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@kriszu/eslint-config": "^3.2.12",
    "@types/color": "^3.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.16.7",
    "@types/vscode": "~1.85.0",
    "color": "^4.2.3",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.1",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.2",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1"
  }
}
