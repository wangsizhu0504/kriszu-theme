{
  "publisher": "wangsizhu0504",
  "name": "kriszu-theme",
  "displayName": "Kriszu VScode",
  "type": "module",
  "version": "1.0.18",
  "description": "custom vitesse theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangsizhu0504/kriszu-theme"
  },
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "categories": [
    "Themes"
  ],
  "icon": "logo.png",
  "files": [
    "LICENSE",
    "extra/*",
    "logo.png",
    "themes/*"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "preview": true,
  "contributes": {
    "themes": [
      {
        "label": "Kriszu Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/kriszu-dark-soft.json"
      },
      {
        "label": "Kriszu Light",
        "uiTheme": "vs",
        "path": "./themes/kriszu-light.json"
      },
      {
        "label": "Kriszu Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/kriszu-dark.json"
      },
      {
        "label": "Kriszu Light Soft",
        "uiTheme": "vs",
        "path": "./themes/kriszu-light-soft.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "esno watch src/index.ts",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "vsce package",
    "release": "vsce publish --no-dependencies",
    "pack": "vsce pack --no-dependencies"
  },
  "devDependencies": {
    "@kriszu/eslint-config": "^4.0.2",
    "@types/color": "^4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.8.1",
    "@vscode/vsce": "^3.6.2",
    "color": "^5.0.2",
    "eslint": "^9.38.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.2",
    "typescript": "^5.9.3",
    "xterm": "^5.3.0"
  }
}
